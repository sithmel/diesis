{
  "name": "diesis",
  "version": "1.0.1",
  "description": "A declarative dependency injection library",
  "main": "es2015/index",
  "module": "es2015/index.mjs",
  "jsnext:main": "es2015/index.mjs",
  "scripts": {
    "test": "npm run clean && npm run build:es2015-cjs && mocha",
    "lint": "eslint --fix --ext .js ./src ./test",
    "clean": "rimraf es5 es2015",
    "build": "npm run clean && npm run build:es5 && npm run build:es5-cjs && npm run build:es2015 && npm run build:es2015-cjs",
    "build:es5": "cross-env BABEL_ENV=es5 babel -x \".mjs\" src -d es5 && renamer --regex --find '.js$' --replace '.mjs' 'es5/**'",
    "build:es5-cjs": "cross-env BABEL_ENV=es5-cjs babel -x \".mjs\" src -d es5",
    "build:es2015": "cross-env BABEL_ENV=es2015 babel -x \".mjs\" src -d es2015 && renamer --regex --find '.js$' --replace '.mjs' 'es2015/**'",
    "build:es2015-cjs": "cross-env BABEL_ENV=es2015-cjs babel -x \".mjs\" src -d es2015",
    "prepublish": "npm run build",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "keywords": [
    "plugins",
    "dependency injection"
  ],
  "repository": "git@github.com:sithmel/diesis.git",
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-entries": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "mocha": "^5.0.1",
    "renamer": "^0.7.3",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "little-ds-toolkit": "^1.0.0"
  }
}
